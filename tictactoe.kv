#:kivy 1.0.9

<Oh>:
    size: 50, 50 
    canvas:
        Ellipse:
            pos: self.pos
            size: self.size          

<Ex>:
    size: 50, 50
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size

<Area>:
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size

<TicTacToeGame>:
    player1: player_ex
    player2: player_oh
    
